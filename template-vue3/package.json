{
  "name": "vue-dsfr-project",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build": "run-p clean:dist build-only",
    "build-only": "vite build",
    "dev": "vite",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format:prettier": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "vite preview --host --port 5050",
    "test": "run-s test:unit test:ct:ci build test:e2e:ci",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.10.1",
    "@gouvminint/vue-dsfr": "^4.6.0",
    "focus-trap": "^7.5.2",
    "focus-trap-vue": "^4.0.2",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@cypress/vue": "^6.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.15",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.0",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.4",
    "workbox-window": "^7.0.0"
  }
}